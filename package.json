{
  "name": "node-starter",
  "version": "0.0.0",
  "description": "A node app starter",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest --globals",
    "test:run": "vitest run --globals",
    "release": "pnpm build && bumpp --tag --push --no-verify && pnpm publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.7.8",
    "@yangss/eslint-config-ts": "^0.2.12",
    "@yangss/lint-msg": "^0.0.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.22.0",
    "tsx": "^3.8.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --fix"
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "Nicholas Yang <yss_2016@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yangss3/node-starter.git"
  },
  "homepage": "https://github.com/yangss3/node-starter#readme",
  "license": "MIT"
}
